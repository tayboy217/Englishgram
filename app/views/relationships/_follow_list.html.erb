 <% if users.exists? %>
  <% users.each do |user| %>
    <table>
      <thead>
        <tr>
          <th>NAME</th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td> <%= link_to user.username,user_path(user),class: "btn btn-outline-primary"%></td>
          <td>フォロー数: <%= user.followings.count %></td>
          <td>フォロワー数: <%= user.followers.count %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
  <% else %>
  <p>ユーザーはいません</p>
<% end %>